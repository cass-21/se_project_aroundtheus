{
    "editor.wordWrap": "on",
    "editor.fontFamily": "'Poppins', sans-serif",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabCompletion": "on",
    "workbench.colorTheme": "Andromeda Colorizer",
    "workbench.preferredDarkColorTheme": "Andromeda Colorizer",
    "workbench.iconTheme": "vscode-icons",
    "workbench.editor.highlightModifiedTabs": true,
    "Codegeex.Chat.LanguagePreference": "English",
    "html.format.indentInnerHtml": true,
    "html-css-class-completion.enableEmmetSupport": true,
    "tailwindCSS.emmetCompletions": true,
    "liveServer.settings.NoBrowser": false,
    "liveServer.settings.showOnStatusbar": true,
    "liveServer.settings.CustomBrowser": "chrome",
    "liveServer.settings.port": 5500,
    "workbench.layoutControl.type": "both",
    "window.density.editorTabHeight": "default",
    "workbench.editor.editorActionsLocation": "titleBar",
    "workbench.layoutControl.enabled": true,
    "accessibility.hideAccessibleView": true,
    "scm.alwaysShowActions": false,
    "terminal.integrated.accessibleViewFocusOnCommandExecution": false,
    "terminal.integrated.allowChords": false,
    "task.quickOpen.showAll": false,
    "notebook.experimental.variablesView": false,
    "notebook.lineNumbers": "on",
    "notebook.insertToolbarLocation": "both",
    "pieces.autocomplete": true,
    "files.autoSave": "onFocusChange",
    "[html]": {
        "editor.minimap.autohide": false,
        "diffEditor.codeLens": false,
        "diffEditor.experimental.showMoves": false,
        "editor.formatOnSave": false,
        "editor.formatOnPaste": false,
        "editor.formatOnType": true
    },
    "terminal.integrated.focusAfterRun": "terminal",
    "notebook.cellFocusIndicator": "border",
    "notebook.cellToolbarVisibility": "hover",
    "notebook.outline.showCodeCells": false,
    "git.enableStatusBarSync": true,
    "editor.suggest.showStatusBar": false,
    "workbench.statusBar.visible": true,
    "zenMode.hideStatusBar": true,
    "debug.enableStatusBarColor": false,
    "debug.showInStatusBar": "onFirstSessionStart",
    "problems.showCurrentInStatus": false,
    "search.experimental.closedNotebookRichContentResults": false,
    "editor.suggest.shareSuggestSelections": true,
    "workbench.panel.opensMaximized": "always",
    "search.quickOpen.includeSymbols": false,
    "debug.toolBarLocation": "docked",
    "emmet.triggerExpansionOnTab": true
}