{
    "editor.wordWrap": "on",
    "editor.fontFamily": "'Poppins', sans-serif",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabCompletion": "off",
    "workbench.colorTheme": "Andromeda Colorizer",
    "workbench.preferredDarkColorTheme": "Andromeda Colorizer",
    "workbench.iconTheme": "vscode-icons",
    "workbench.editor.highlightModifiedTabs": true,
    "Codegeex.Chat.LanguagePreference": "English",
    "html.format.indentInnerHtml": true,
    "html-css-class-completion.enableEmmetSupport": true,
    "tailwindCSS.emmetCompletions": true,
    "liveServer.settings.NoBrowser": false,
    "liveServer.settings.showOnStatusbar": true,
    "liveServer.settings.CustomBrowser": "chrome",
    "liveServer.settings.port": 5500,
    "workbench.layoutControl.type": "toggles",
    "window.density.editorTabHeight": "default",
    "workbench.editor.editorActionsLocation": "titleBar",
    "workbench.layoutControl.enabled": true,
    "accessibility.hideAccessibleView": true,
    "scm.alwaysShowActions": false,
    "terminal.integrated.accessibleViewFocusOnCommandExecution": true,
    "terminal.integrated.allowChords": false,
    "task.quickOpen.showAll": false,
    "notebook.experimental.variablesView": false,
    "notebook.lineNumbers": "on",
    "notebook.insertToolbarLocation": "both",
    "pieces.autocomplete": true
}